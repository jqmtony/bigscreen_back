<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gochinatv.accelarator.dao.AppAdSettingsDao">

	<resultMap type="AppAdSettings" id="AppAdSettings">
		<id property="id" column="id" />
		<result property="adIndex" column="ad_index" />
		<result property="packageId" column="package_id" />
		<result property="type" column="type" />
		<result property="categoryId" column="category_id" />
	</resultMap>
	
	<select id="getList" resultMap="AppAdSettings">
		select * from app_ad_settings
	</select>

</mapper>